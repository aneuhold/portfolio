{
  "name": "svelte",
  "version": "0.0.1",
  "license": "ISC",
  "scripts": {
    "dev": "vite dev --port 3020 --open",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "test": "jest",
    "buildAndTest": "yarn build && yarn test",
    "upgradeAndTest": "yarn upgrade --latest && yarn buildAndTest"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "1.0.0-next.72",
    "@sveltejs/kit": "1.0.0-next.480",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.1",
    "@types/jest": "^29.0.0",
    "@types/marked": "^4.0.7",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "babel-jest": "^29.0.3",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "sass": "^1.54.9",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.0",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.6",
    "ts-jest": "^29.0.0",
    "tslib": "^2.3.1",
    "typescript": "~4.8.3",
    "vite": "^3.1.0"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.42",
    "marked": "^4.1.0",
    "mdsvex": "^0.10.6",
    "node-sass": "^7.0.3"
  }
}
